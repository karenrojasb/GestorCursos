<td className="border p-2">
  {(() => {
    const profesores = [curso.NombreProfesor, curso.SegundoPro, curso.Proexterno]
      .filter((profesor) => typeof profesor === "string" && profesor.trim() !== "");

    return profesores.length > 0 ? (
      <ul>
        {profesores.map((profesor, index) => (
          <li key={index}>{profesor}</li>
        ))}
      </ul>
    ) : (
      <span>Sin profesor asignado</span>
    );
  })()}
</td>