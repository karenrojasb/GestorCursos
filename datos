{(() => {
  const notaEncontrada = nota.find(n => 
    String(n.idInscrito).trim().toLowerCase() === String(inscripciones.docInscr).trim().toLowerCase()
  );

  if (!notaEncontrada || !notaEncontrada.Listas || !notaEncontrada.Listas.Especificacion || notaEncontrada.Listas.Especificacion === "Sin calificar") {
    return "Sin Nota";
  }

  return notaEncontrada.Listas.Especificacion;
})()}