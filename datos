async actualizarInscripcion(id: number, dto: UpdateInscripcionDto) {
  return this.prisma.inscripciones.update({
    where: { id },
    data: {
      est: dto.est !== undefined ? Boolean(dto.est) : undefined,
      calificacion: dto.calificacion !== undefined ? Boolean(dto.calificacion) : undefined,
    },
  });
}