useEffect(() => {
  const fetchNotasPorInscritos = async () => {
    try {
      const todasLasNotas: any[] = [];

      for (const inscripcion of inscripciones) {
        const response = await fetch(`http://localhost:8090/api/Notas/${inscripcion.docInscr}`);
        if (!response.ok) throw new Error(`Error al obtener notas para ${inscripcion.docInscr}`);
        const data = await response.json();
        todasLasNotas.push(...data);
      }

      console.log("Notas individuales recibidas:", todasLasNotas);
      setNota(todasLasNotas);
    } catch (error) {
      console.error("Error al obtener notas individuales:", error);
    }
  };

  if (inscripciones.length > 0) {
    fetchNotasPorInscritos();
  }
}, [inscripciones]);