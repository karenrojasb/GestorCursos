<tbody>
  {inscripciones.map((inscripcion: any) => (
    <tr key={inscripcion.id} className="text-center hover:bg-gray-100 transition">
      <td className="border p-2">{inscripcion.id}</td>
      <td className="border p-2">
        {inscripcion.curso?.nombre_curso ? inscripcion.curso.nombre_curso : "Desconocido"}
      </td>
      <td className="border p-2">{inscripcion.docInscr}</td>
      <td className="border p-2">{inscripcion.est}</td>
      <td className="border p-2">{new Date(inscripcion.fecreg).toLocaleDateString()}</td>
    </tr>
  ))}
</tbody>