async obtenerCursosPorId(idInscrito: number) {
  const notas = await this.prisma.notas.findMany({
    where: {
      idInscrito: idInscrito,
      Nota: 33,
    },
    include: {
      Listas: {
        select: {
          Especificacion: true,
        },
      },
      Inscripciones: {
        select: {
          docInscr: true,
          nombre: true,
          idCur: true,
        },
      },
    },
  });

  return notas.map(nota => ({
    nombre: nota.Inscripciones?.nombre || 'Desconocido',
    documento: nota.Inscripciones?.docInscr || 'Sin documento',
    idCurso: nota.Inscripciones?.idCur || 'Sin curso',
    especificacion: nota.Listas?.Especificacion || 'Sin especificaci√≥n',
  }));
}



@Get(':idInscrito')
async obtenerCursosPorId(@Param('idInscrito') idInscrito: string) {
  return this.notasService.obtenerCursosPorId(idInscrito);
}
