model Inscripciones {
  id           Int       @id(map: "PK__Inscripc__3213E83FF1560A49") @default(autoincrement())
  idCur        Int?
  docInscr     Int?
  est          Boolean?
  fecreg       DateTime? @db.Date
  calificacion Boolean   @default(false, map: "DF__Inscripci__calif__0F624AF8")
  Cursos       Cursos?   @relation(fields: [idCur], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK__Inscripci__idCur__76969D2E")
  Notas        Notas[]

  @@schema("gescur")
}

model Listas {
  id                                Int      @id(map: "PK__Listas__3213E83F66C0FB4F") @default(autoincrement())
  Especificacion                    String?  @db.VarChar(50)
  Tipo                              Int?
  EspecificacionTipo                String?  @db.VarChar(50)
  DependenciaTipo                   Int?
  Cursos_Cursos_EstadoToListas      Cursos[] @relation("Cursos_EstadoToListas")
  Cursos_Cursos_IdTipoCursoToListas Cursos[] @relation("Cursos_IdTipoCursoToListas")
  Cursos_Cursos_LineaToListas       Cursos[] @relation("Cursos_LineaToListas")
  Cursos_Cursos_ModalidadToListas   Cursos[] @relation("Cursos_ModalidadToListas")
  Cursos_Cursos_PublicoToListas     Cursos[] @relation("Cursos_PublicoToListas")
  Notas                             Notas[]

  @@schema("gescur")
}

model Notas {
  id            Int            @id(map: "PK__Notas__3213E83FC9F3E9E8") @default(autoincrement())
  idCurso       Int?
  idInscrito    Int?
  Nota          Int?
  idRegistro    Int?
  FechaRegistro DateTime?      @db.Date
  Inscripciones Inscripciones? @relation(fields: [idCurso], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK__Notas__idCurso__7F2BE32F")
  Listas        Listas?        @relation(fields: [Nota], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK__Notas__Nota__00200768")

  @@schema("gescur")
}
