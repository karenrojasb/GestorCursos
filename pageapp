async obtenerInscripcionesPorId(idInscrito: number) {
  const notas = await this.prisma.notas.findMany({
    where: {
      idInscrito: idInscrito,
      Nota: 33,
    },
    include: {
      Listas: {
        select: {
          Especificacion: true,
        },
      },
      Inscripciones: {
        select: {
          docInscr: true,
          idCur: true,
        },
      },
    },
  });

  return notas.map(nota => ({
    documento: nota.Inscripciones?.docInscr ?? 'Desconocido',
    cursoId: nota.Inscripciones?.idCur ?? 'Sin curso',
    especificacion: nota.Listas?.Especificacion ?? 'Sin especificaci√≥n',
  }));
}