// OBTENER NOTAS POR IDINSCRITO CON ESPECIFICACION
async obtenerPorIdInscrito(idInscrito: number) {
  const notas = await this.prisma.notas.findMany({
    where: {
      idInscrito: idInscrito,
    },
    include: {
      Listas: {
        select: {
          Especificacion: true,
        },
      },
    },
  });

  return notas;
}



@Get(':idInscrito')
async obtenerPorIdInscrito(@Param('idInscrito') idInscrito: string) {
  return this.notasService.obtenerPorIdInscrito(Number(idInscrito));
}